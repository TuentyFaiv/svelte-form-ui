{
	"name": "@tuentyfaiv/svelte-form-ui",
	"version": "0.0.3",
	"description": "Built-in forms for Svelte using @tuentyfaiv/svelte-form",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"keywords": [
		"Svelte",
		"Forms",
		"UI",
		"Builtin Forms"
	],
	"author": "Tonalli LÃ³pez <contact@tuentyfaiv.com>",
	"license": "MIT",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"peerDependencies": {
		"@emotion/css": "^11.11.2",
		"@tuentyfaiv/svelte-form": "^0.2.8",
		"svelte": "^4.0.5",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@emotion/css": "^11.11.2",
		"@playwright/test": "^1.36.0",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@sveltejs/package": "^2.2.0",
		"@tuentyfaiv/svelte-form": "^0.2.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.44.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-module-utils": "^2.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte": "^2.32.2",
		"eslint-scope": "^7.2.0",
		"eslint-visitor-keys": "^3.4.1",
		"publint": "^0.1.16",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"svelte-eslint-parser": "^0.32.1",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.3",
		"vitest": "^0.33.0",
		"yup": "^1.2.0"
	},
	"engines": {
		"npm": ">=7.0.0",
		"pnpm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TuentyFaiv/svelte-form-ui.git"
	},
	"bugs": {
		"url": "https://github.com/TuentyFaiv/svelte-form-ui/issues"
	},
	"homepage": "https://github.com/TuentyFaiv/svelte-form-ui#readme",
	"main": "./dist/index.js",
	"directories": {
		"test": "tests"
	}
}
